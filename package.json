    "antd": "3.23.6",
    "antd-mask-input": "^0.1.12",
    "aws-amplify": "^3.0.10",
    "aws-sdk": "2.549.0",
    "axios": "0.19.0",
    "babel-plugin-import": "1.12.2",
    "customize-cra": "0.8.0",
    "file-saver": "^2.0.2",
    "final-form": "4.18.5",
    "final-form-arrays": "3.0.1",
    "flow-bin": "0.109.0",
    "history": "4.10.1",
    "husky": "3.0.9",
    "jest-canvas-mock": "2.1.2",
    "leaflet": "^1.6.0",
    "less": "3.10.3",
    "less-loader": "5.0.0",
    "lint-staged": "9.4.2",
    "mobx": "5.14.0",
    "mobx-react": "5.4.3",
