    "react-final-form": "6.3.0",
    "react-final-form-arrays": "3.1.1",
    "react-final-form-listeners": "1.0.2",
    "react-input-mask": "2.0.4",
    "react-leaflet": "^2.6.1",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.2.0",
    "resize-base64": "1.0.12",
