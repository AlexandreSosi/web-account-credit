{
  "name": "web-account-credit",
  "jest": {
    "testMatch": [
      "**src/**/*.test.js"
    ],
    "moduleNameMapper": {
      "^~/(.*)": "<rootDir>/src/$1"
    },
    "collectCoverageFrom": [
      "!**/src/__mocks__/**",
      "!**/src/__tests__/**",
      "!**/src/commons/**",
      "!**/src/components-form/**",
      "!**/src/form-validators/**",
      "!**/src/layout/**",
      "!**/src/layouts/**",
      "!**/src/providers/**",
      "!**/src/routes/**",
      "!**/src/services/**",
      "!**/src/stores/**",
      "!**/src/styles/**",
      "!**/src/utils/**",
      "!**/src/App.js",
      "!**/src/AppEnvironment.js",
      "!**/src/index.js",
      "!**/src/serviceWorker.js"
    ],
    "coverageDirectory": "src/tests/coverage"
  },
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@aws-amplify/ui-react": "^0.2.5",
    "@fortawesome/fontawesome-svg-core": "1.2.25",
